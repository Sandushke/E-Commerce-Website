@import url('https://fonts.googleapis.com/css2?family=Poppins&display=swap');

body{
	background-image:url("background.jpg")
}
#MainHeading{
	font-size:50px;
	text-align:center;
	padding-top:20px;
	padding-bottom:60px;
}

#offerImage{
	width:50%;
	text-align:center;
	margin-left:auto;
	margin-right:auto;
	padding-bottom:50px;
	display:block;
}
.images{
	float:left;
	padding:5px;
	width:19.2%;
	height:20%;
	background-size:cover;
	opacity:100%;
}

.centered{
	position:relative;
	bottom:140px;
	right:-55px;
	background-color:transparent;
	color:#ffffff;
	padding-left:20px;
	padding-right:20px;
}
.Categories::after{
	content:"";
	clear:both;
	display:table;
}
